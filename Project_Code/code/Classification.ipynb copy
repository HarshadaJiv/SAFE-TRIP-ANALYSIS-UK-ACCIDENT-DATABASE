    "y = [162341,151955,144552] \n",
    "  \n",
    "# plotting the points  \n",
    "plt.plot(x, y) \n",
    "  \n",
    "# naming the x axis \n",
    "plt.xlabel('Year') \n",
    "# naming the y axis \n",
    "plt.ylabel('Number of Accidents That Happened') \n",
    "  \n",
    "# giving a title to my graph \n",
    "plt.title('Accidents in a Year') \n",
    "  \n",
    "# function to show the plot \n",
    "plt.show() "
   ]
