    "\n",
    "predictor_cols = ['Vehicle_Type',\n",
    "'Towing_and_Articulation',\n",
    "'Vehicle_Manoeuvre',\n",
    "'Vehicle_Location-Restricted_Lane',\n",
    "'Junction_Location',\n",
